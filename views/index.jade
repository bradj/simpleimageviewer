doctype 5
html(lang="en")
    head
        title= title
        include includes/resources.jade
    body(style="padding-top: 70px;")
        div.container
            ul.nav.nav-pills.nav-stacked.col-lg-4.col-lg-offset-4.col-md-6.col-md-offset-3.col-sm-6.col-sm-offset-3
                each item in items
                    li
                        a(href=item.prefix).btn.btn-primary.btn-lg= item.title